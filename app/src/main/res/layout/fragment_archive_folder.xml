<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".archive.ArchiveFolderFragment">
    <data>
        <variable
            name="ArchiveFolderFragment"
            type="com.example.kks.archive.ArchiveFolderFragment"/>
    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <LinearLayout
            android:layout_width="251dp"
            android:layout_height="396dp"
            android:layout_gravity="center"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="archivefolder입니당" />

            <Button
                android:id="@+id/goprofilebtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="@{ArchiveFolderFragment::goProfile}"
                android:text="profile확인" />

            <Button
                android:id="@+id/gopatternbtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="@{ArchiveFolderFragment::goPattern}"
                android:text="문화패턴" />

        </LinearLayout>

    </FrameLayout>
</layout>
